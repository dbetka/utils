{
  "extends": [
    "plugin:vue/essential",
    "standard"
  ],
  "rules": {
    "no-undef": "off",
    "eqeqeq": "warn",
    "no-unused-vars": "warn",
    "no-useless-call": "warn",
    "no-useless-escape": "warn",
    "semi": [
      "error",
      "always"
    ],
    "comma-dangle": [
      "error",
      "always-multiline"
    ],
    "quote-props": [
      "error",
      "consistent"
    ],
    "indent": [
      "error",
      2
    ],
    "vue/html-indent": [
      "error",
      2
    ],
    "vue/script-indent": [
      "error",
      2
    ]
  }
}
